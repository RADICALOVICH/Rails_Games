<% if user_signed_in? %>
  <% if current_user.wishlist.include?(@str.to_i)%>
    <%= button_to "Удалить из желаемого", games_delete_game_path, params: { game: @str.to_i }, class: 'btn btn-danger'%>
  <% else %>
    <%= button_to "Добавить в желаемое", games_add_game_path,params: { game: @str.to_i }, class: 'btn btn-primary'%>
  <% end %>
<% end %>